 auto}.affiliate-disclaimer.hidden{display:none}.affiliate-disclaimer{border-bottom:.0625rem solid #e9e9e9}.affiliate-disclaimer p{font-family:Charter,Georgia,Times,Serif;font-size:.75rem;line-height:1.3;font-style:italic;color:#aaa;padding-left:.625rem}.mobile-adhesion-unit{background-color:#e2e2e2;position:fixed;bottom:0;width:100%;text-align:center;z-index:1009}@media only screen and (min-width:61.25rem){.mobile-adhesion-unit{display:none}}.mobile-adhesion-unit-close-button{background-color:#000;border-radius:50%;color:#fff;display:none;font-size:.75rem;height:1.25rem;line-height:1.25rem;padding:0;position:absolute;right:10px;top:-9px;width:1.25rem;-webkit-appearance:none}.mobile-adhesion-unit-close-button.active{display:block}.mobile-adhesion-unit-close-button:empty{display:none}#adAdhesion{margin:0 auto;position:relative;text-align:center}
